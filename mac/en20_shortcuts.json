{
  "title": "en20 - shortcuts",
  "rules": [
    {
      "description": "en20 系统: alt+tab 切换应用",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "mandatory": ["left_control"]
            }
          },
          "to": [{
            "key_code": "tab",
            "modifiers": ["left_command"]
          }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "`",
            "modifiers": {
              "mandatory": ["left_control"]
            }
          },
          "to": [{
            "key_code": "`",
            "modifiers": ["left_command"]
          }]
        }
      ]
    },
    {
      "description": "en20 浏览器: ,f5刷新,f12,ctrl+w ",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [{
            "key_code": "w",
            "modifiers": ["left_command"]
          }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f12",
            "modifiers": {"optional": ["any"]}
          },
          "to": [{
            "key_code": "i",
            "modifiers": ["left_command", "left_option"]
          }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f5",
            "modifiers": {"optional": ["any"]}
          },
          "to": [{
            "key_code": "r",
            "modifiers": ["left_command"]
          }]
        }
      ]
    }
  ]
}
