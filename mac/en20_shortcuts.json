{
  "title": "en20 - shortcuts",
  "rules": [
    {
      "description": "en20 的常用快捷键",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [{
            "key_code": "w",
            "modifiers": ["left_command"]
          }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f12",
            "modifiers": {"optional": ["any"]}
          },
          "to": [{
            "key_code": "i",
            "modifiers": ["left_command", "left_option"]
          }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f5",
            "modifiers": {"optional": ["any"]}
          },
          "to": [{
            "key_code": "r",
            "modifiers": ["left_command"]
          }]
        }
      ]
    }
  ]
}
